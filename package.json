    "@nuxtjs/tailwindcss": "6.12.1",
    "@supabase/supabase-js": "^2.45.4",
    "nuxt": "^3.14.159",
    "nuxt-svgo": "^4.0.6",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@histoire/plugin-nuxt": "^0.17.17",
    "@histoire/plugin-vue": "^0.17.17",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "histoire": "^0.17.17",
    "prettier": "3.3.3",
